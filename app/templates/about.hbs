<a name="top"></a>
<div class="banner mdl-typography--text-center">
    <div class="logo-font slogan">
        <div>Ideas are cheap, execution is everything</div>
    </div>
    <div class="logo-font sub-slogan">Become an Ideator and share your ideas around the world</div>
</div>
<div class="mdl-grid entry-content">
{{#each model.aboutList as | about index|}}
    <div class="featureBlock">
        <div class="mdl-card__actions mdl-card--border"></div>
        <div class="item">
        <img class="image" src="{{about.image}}">
        <div class="caption">
            <h3>{{about.title}}</h3>
            <p>{{about.description}}</p></div>
        </div>
    </div>
{{/each}}
</div>
